; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Conversation Simulator"
run/main_scene="res://Scenes/Screens/MainMenu.tscn"
boot_splash/bg_color=Color( 0.00392157, 0, 0.101961, 1 )
config/icon="res://Resources/Images/Icon.png"
config/windows_native_icon="res://Resources/Images/Icon.ico"

[audio]

default_bus_layout="res://Materials/BusLayout.tres"

[autoload]

Globals="*res://Scripts/Globals.cs"
SceneManager="*res://Scripts/Scenes/SceneManager.cs"

[display]

window/size/height=768
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[mono]

project/assembly_name="Conversation Simulator"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
